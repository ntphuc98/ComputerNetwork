import java.io.IOException;
import java.net.URL;
import java.net.URLConnection;

public class Bai2 {
	public static URL url;
	public static URLConnection uc;

	public Bai2(URL url) {
		Bai2.url = url;
		Bai2.uc = uc;
	}
	public void content() {
		try {
			uc = url.openConnection();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
